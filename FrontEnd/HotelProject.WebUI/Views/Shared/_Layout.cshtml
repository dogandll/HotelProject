<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/PartialViews/_HeadPartial.cshtml")

<body> 
    @await Html.PartialAsync("/Views/PartialViews/_PreloaderPartial.cshtml") 
    <div id="main-wrapper"> 
        @await Html.PartialAsync("/Views/PartialViews/_NavHeaderPartial.cshtml") 
        @await Html.PartialAsync("/Views/PartialViews/_HeaderPartial.cshtml") 
        @await Html.PartialAsync("/Views/PartialViews/_SideBarPartial.cshtml") 
        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body"> 
            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div> 
        </div>
        @await Html.PartialAsync("/Views/PartialViews/_FooterPartial.cshtml")
    </div>
    @await Html.PartialAsync("/Views/PartialViews/_ScriptPartial.cshtml")
</body> 
</html>